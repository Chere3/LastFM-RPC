{
  "name": "music-rpc",
  "version": "1.0.0",
  "description": "A discord rich presence that shows the music im listening, fetching the last.fm API.",
  "scripts": {
    "dev": "yarn ts-node-dev --respawn --transpile-only ./src/index.ts dev",
    "start": "tsc -p tsconfig.json && node /dist/index.js",
    "lint": "npx eslint src/",
    "build": "tsc -p tsconfig.json"
  },
  "main": "dist/index.js",
  "author": "Cheree",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "discord-rpc": "^4.0.1",
    "eslint": ">=8.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@typescript-eslint/eslint-plugin": ">=5.43.0",
    "@typescript-eslint/parser": ">=5.43.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^0.56.0",
    "husky": "^8.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": ">=4.4"
  }
}
